<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>

<body>

    <table class="table" align="center"  style="width: 800px;">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Contact</th>
                <th scope="col">action</th>
            </tr>
        </thead>
        <tbody>
            <% students.forEach(function(student){ %>
                <tr>
                    <th><%= student.id %></th>
                    <td><%= student.name %></td>
                    <td><%= student.pass %></td>
                    <td>
                        <button class="btn btn-success" type="edit">
                            <a class="text-dark text-decoration-none" href="/edit/<%= student.id %>">Edit</a>
                        </button>
                    ||
                        <button class="btn btn-danger" type="delete">
                            <a class="text-dark text-decoration-none" href="/delete/<%= student.id %>">Delete</a>
                        </button>
                    </td>
                </tr>
              <% }); %>
        </tbody>
    </table>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
</body>

</html>